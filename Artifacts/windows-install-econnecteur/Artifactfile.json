{
    "$schema": "https://raw.githubusercontent.com/Azure/azure-devtestlab/master/schemas/2016-11-28/dtlArtifacts.json",
    "title": "Install E-Connecteur",
    "description": "Use this to download and install latest E-Connecteur stable version",
    "publisher": "Vaisonet",
    "tags": [
        "EC"
    ],
    "iconUri": "https://raw.githubusercontent.com/Azure/azure-devtestlab/master/Artifacts/windows-run-powershell/powershell.png",
    "targetOsType": "Windows",
    
        
        "runCommand": {

            "commandToExecute": "[concat('powershell.exe -ExecutionPolicy bypass \"&', 'Invoke-WebRequest -Uri https://builds.vaisonet.com/liveupdate_server/update-econnecteur.exe', '-OutFile C:\\ECO\\update-econnecteur.exe, '-TimeoutSec 60', '\"')]"

        },

        "secondrunCommand": {

            "commandToExecute": "[concat('powershell.exe -ExecutionPolicy bypass \"&', 'Start-Process C:\\ECO\\update-econnecteur.exe', '-ArgumentList \"/SILENT\", \"/NOCANCEL\", \"/NORESTART\", \"/FORCECLOSEAPPLICATIONS\"', ' -Wait', '\"')]"

        }
    }
        